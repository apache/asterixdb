DataverseUse test
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [tbin]
    :
    FieldAccessor [
      Variable [ Name=$gen0 ]
      Field=bin
    ]
  )
  (
    LiteralExpr [STRING] [interval]
    :
    Variable [ Name=$itv ]
  )
  (
    LiteralExpr [STRING] [overlap]
    :
    FunctionCall asterix.get-overlapping-interval@2[
      FieldAccessor [
        Variable [ Name=$gen0 ]
        Field=bin
      ]
      Variable [ Name=$itv ]
    ]
  )
]
]
FROM [  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [bin]
        :
        Variable [ Name=$bin ]
      )
      (
        LiteralExpr [STRING] [i]
        :
        (
          SELECT ELEMENT [
          FieldAccessor [
            Variable [ Name=$g ]
            Field=i
          ]
          ]
          FROM [            Variable [ Name=$g ]
            AS Variable [ Name=$g ]
          ]
        )
      )
    ]
    ]
    FROM [      (
        SELECT ELEMENT [
        Variable [ Name=$i ]
        ]
        FROM [          FunctionCall asterix.dataset@1[
            LiteralExpr [STRING] [test.tsdata]
          ]
          AS Variable [ Name=$i ]
        ]
        Orderby
          FieldAccessor [
            Variable [ Name=$i ]
            Field=time
          ]
          ASC

      )
      AS Variable [ Name=$i ]
,
      FunctionCall asterix.overlap-bins@3[
        FunctionCall asterix.interval-start-from-time@2[
          FieldAccessor [
            Variable [ Name=$i ]
            Field=time
          ]
          FieldAccessor [
            Variable [ Name=$i ]
            Field=duration
          ]
        ]
        FunctionCall asterix.time@1[
          LiteralExpr [STRING] [00:00:00]
        ]
        FunctionCall asterix.day-time-duration@1[
          LiteralExpr [STRING] [PT1H30M]
        ]
      ]
      AS Variable [ Name=$j ]
    ]
    Groupby
      Variable [ Name=$bin ]
      :=
      Variable [ Name=$j ]
      GROUP AS Variable [ Name=$g ]
      (
        i:=Variable [ Name=$i ]
        j:=Variable [ Name=$j ]
      )

    Orderby
      FunctionCall asterix.get-interval-start@1[
        Variable [ Name=$bin ]
      ]
      ASC

  )
  AS Variable [ Name=$gen0 ]
,
  FieldAccessor [
    Variable [ Name=$gen0 ]
    Field=i
  ]
  AS Variable [ Name=$x ]
]
Let Variable [ Name=$itv ]
  :=
  FunctionCall asterix.interval-start-from-time@2[
    FieldAccessor [
      Variable [ Name=$x ]
      Field=time
    ]
    FieldAccessor [
      Variable [ Name=$x ]
      Field=duration
    ]
  ]
Orderby
  FunctionCall asterix.get-interval-start@1[
    FieldAccessor [
      Variable [ Name=$gen0 ]
      Field=bin
    ]
  ]
  ASC
  Variable [ Name=$itv ]
  ASC
  FunctionCall asterix.get-overlapping-interval@2[
    FieldAccessor [
      Variable [ Name=$gen0 ]
      Field=bin
    ]
    Variable [ Name=$itv ]
  ]
  ASC

