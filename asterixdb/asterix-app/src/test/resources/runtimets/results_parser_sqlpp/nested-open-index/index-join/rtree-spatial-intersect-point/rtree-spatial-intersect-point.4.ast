      FieldAccessor [
        FieldAccessor [
          Variable [ Name=$a ]
          Field=nested
        ]
        Field=id
      ]
      !=
      FieldAccessor [
        FieldAccessor [
          Variable [ Name=$b ]
          Field=nested
        ]
        Field=id
      ]
    ]
  ]
Orderby
