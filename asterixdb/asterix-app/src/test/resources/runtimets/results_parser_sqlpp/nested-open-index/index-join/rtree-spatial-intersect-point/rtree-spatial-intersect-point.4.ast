    ]
  )
  (
    LiteralExpr [STRING] [bid]
    :
    FieldAccessor [
      FieldAccessor [
        Variable [ Name=$b ]
        Field=nested
      ]
      Field=id
    ]
  )
  (
    LiteralExpr [STRING] [apt]
    :
    FieldAccessor [
      FieldAccessor [
        Variable [ Name=$a ]
        Field=nested
      ]
      Field=point
    ]
  )
  (
    LiteralExpr [STRING] [bp]
    :
    FieldAccessor [
      FieldAccessor [
        Variable [ Name=$b ]
        Field=nested
      ]
      Field=point
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.MyData1]
