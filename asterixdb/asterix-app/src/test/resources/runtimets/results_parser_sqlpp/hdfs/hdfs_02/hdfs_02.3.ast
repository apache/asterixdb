DataverseUse test
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [word]
    :
    Variable [ Name=$tok ]
  )
  (
    LiteralExpr [STRING] [count]
    :
    FunctionCall asterix.count@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          Variable [ Name=$g ]
          Field=token
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$g ]
        ]
      )
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.TextDataset]
  ]
  AS Variable [ Name=$line ]
,
  FunctionCall asterix.word-tokens@1[
    FieldAccessor [
      Variable [ Name=$line ]
      Field=content
    ]
  ]
  AS Variable [ Name=$token ]
]
Groupby
  Variable [ Name=$tok ]
  :=
  Variable [ Name=$token ]
  GROUP AS Variable [ Name=$g ]
  (
    line:=Variable [ Name=$line ]
    token:=Variable [ Name=$token ]
  )

Orderby
  Variable [ Name=$tok ]
  ASC

