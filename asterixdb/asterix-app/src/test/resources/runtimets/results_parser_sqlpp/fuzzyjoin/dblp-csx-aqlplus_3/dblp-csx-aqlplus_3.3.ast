DataverseUse fuzzyjoin
Set simthreshold=.5f
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [dblp]
    :
    Variable [ Name=$dblp ]
  )
  (
    LiteralExpr [STRING] [csx]
    :
    Variable [ Name=$csx ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [fuzzyjoin.DBLP]
  ]
  AS Variable [ Name=$dblp ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [fuzzyjoin.CSX]
  ]
  AS Variable [ Name=$csx ]
]
Where
  OperatorExpr [
    FunctionCall fuzzyjoin.word-tokens@1[
      FieldAccessor [
        Variable [ Name=$csx ]
        Field=title
      ]
    ]
    ~=
    FunctionCall fuzzyjoin.word-tokens@1[
      FieldAccessor [
        Variable [ Name=$dblp ]
        Field=title
      ]
    ]
  ]
Orderby
  FieldAccessor [
    Variable [ Name=$dblp ]
    Field=id
  ]
  ASC
  FieldAccessor [
    Variable [ Name=$csx ]
    Field=id
  ]
  ASC

