DataverseUse fuzzyjoin
Set simthreshold=.5f
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [dblp]
    :
    Variable [ Name=$dblp ]
  )
  (
    LiteralExpr [STRING] [csx]
    :
    Variable [ Name=$csx ]
  )
  (
    LiteralExpr [STRING] [dblp2]
    :
    Variable [ Name=$dblp2 ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [fuzzyjoin.DBLP]
  ]
  AS Variable [ Name=$dblp ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [fuzzyjoin.CSX]
  ]
  AS Variable [ Name=$csx ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [fuzzyjoin.DBLP]
  ]
  AS Variable [ Name=$dblp2 ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FunctionCall asterix.word-tokens@1[
        FieldAccessor [
          Variable [ Name=$dblp ]
          Field=title
        ]
      ]
      ~=
      FunctionCall asterix.word-tokens@1[
        FieldAccessor [
          Variable [ Name=$csx ]
          Field=title
        ]
      ]
    ]
    and
    OperatorExpr [
      FunctionCall asterix.word-tokens@1[
        FieldAccessor [
          Variable [ Name=$csx ]
          Field=authors
        ]
      ]
      ~=
      FunctionCall asterix.word-tokens@1[
        FieldAccessor [
          Variable [ Name=$dblp2 ]
          Field=authors
        ]
      ]
    ]
  ]
Orderby
  FieldAccessor [
    Variable [ Name=$dblp ]
    Field=id
  ]
  ASC
  FieldAccessor [
    Variable [ Name=$csx ]
    Field=id
  ]
  ASC
  FieldAccessor [
    Variable [ Name=$dblp2 ]
    Field=id
  ]
  ASC

