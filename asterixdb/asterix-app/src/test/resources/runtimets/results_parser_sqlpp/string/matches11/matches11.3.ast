Query:
SELECT ELEMENT [
Variable [ Name=$a ]
]
FROM [  OrderedListConstructor [
    LiteralExpr [NULL]
    FunctionCall asterix.matches@2[
      LiteralExpr [STRING] [hello]
      LiteralExpr [NULL]
    ]
    FunctionCall asterix.matches@2[
      LiteralExpr [STRING] [hello]
      LiteralExpr [STRING] [helllo]
    ]
    FunctionCall asterix.matches@2[
      LiteralExpr [STRING] [hello]
      LiteralExpr [STRING] [ ]
    ]
    FunctionCall asterix.matches@2[
      LiteralExpr [NULL]
      LiteralExpr [STRING] [hello]
    ]
    FunctionCall asterix.matches@2[
      LiteralExpr [STRING] [hello]
      LiteralExpr [STRING] [[^a-z]]
    ]
  ]
  AS Variable [ Name=$a ]
]
