DataverseUse test
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [arec]
    :
    Variable [ Name=$a ]
  )
  (
    LiteralExpr [STRING] [brec]
    :
    Variable [ Name=$b ]
  )
]
]
FROM [  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [Customers]
  ]
  AS Variable [ Name=$a ]
,
  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [Customers2]
  ]
  AS Variable [ Name=$b ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FunctionCall test.edit-distance@2[
        FieldAccessor [
          Variable [ Name=$a ]
          Field=name
        ]
        FieldAccessor [
          Variable [ Name=$b ]
          Field=name
        ]
      ]
      <=
      LiteralExpr [LONG] [4]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$a ]
        Field=cid
      ]
      <
      FieldAccessor [
        Variable [ Name=$b ]
        Field=cid
      ]
    ]
  ]
Orderby
  FieldAccessor [
    Variable [ Name=$a ]
    Field=cid
  ]
  ASC
  FieldAccessor [
    Variable [ Name=$b ]
    Field=cid
  ]
  ASC

