DataverseUse TinySocial
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [field-name]
    :
    Variable [ Name=$n ]
  )
  (
    LiteralExpr [STRING] [field-type]
    :
    Variable [ Name=$t ]
  )
  (
    LiteralExpr [STRING] [count]
    :
    Variable [ Name=$count ]
  )
]
]
FROM [  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [TweetMessagesAlternate]
  ]
  AS Variable [ Name=$r ]
,
  FunctionCall TinySocial.get-record-fields@1[
    Variable [ Name=$r ]
  ]
  AS Variable [ Name=$f ]
]
Groupby
  Variable [ Name=$n ]
  :=
  FieldAccessor [
    Variable [ Name=$f ]
    Field=field-name
  ]
  Variable [ Name=$t ]
  :=
  FieldAccessor [
    Variable [ Name=$f ]
    Field=field-type
  ]
  GROUP AS Variable [ Name=#1 ]
  (
    f:=Variable [ Name=$f ]
    r:=Variable [ Name=$r ]
  )

Let Variable [ Name=$count ]
  :=
  FunctionCall TinySocial.count@1[
    (
      SELECT ELEMENT [
      FieldAccessor [
        Variable [ Name=#2 ]
        Field=r
      ]
      ]
      FROM [        Variable [ Name=#1 ]
        AS Variable [ Name=#2 ]
      ]
    )
  ]
Orderby
  Variable [ Name=$n ]
  ASC
  Variable [ Name=$t ]
  ASC

