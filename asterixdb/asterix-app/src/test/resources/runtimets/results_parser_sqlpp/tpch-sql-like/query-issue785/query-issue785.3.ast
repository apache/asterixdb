DataverseUse tpch
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [nation_key]
    :
    Variable [ Name=$nation_key ]
  )
  (
    LiteralExpr [STRING] [sum_price]
    :
    (
      SELECT ELEMENT [
      RecordConstructor [
        (
          LiteralExpr [STRING] [orderdate]
          :
          Variable [ Name=$od ]
        )
        (
          LiteralExpr [STRING] [sum_price]
          :
          Variable [ Name=$sum ]
        )
      ]
      ]
      FROM [        Variable [ Name=$g ]
        AS Variable [ Name=$i ]
      ]
      Groupby
        Variable [ Name=$od ]
        :=
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=x
          ]
          Field=order_date
        ]
        GROUP AS Variable [ Name=$g2 ]
        (
          i:=Variable [ Name=$i ]
        )

      Let Variable [ Name=$sum ]
        :=
        FunctionCall asterix.sum@1[
          (
            SELECT ELEMENT [
            FieldAccessor [
              FieldAccessor [
                FieldAccessor [
                  Variable [ Name=$g2 ]
                  Field=i
                ]
                Field=x
              ]
              Field=sum_price
            ]
            ]
            FROM [              Variable [ Name=$g2 ]
              AS Variable [ Name=$g2 ]
            ]
          )
        ]
      Orderby
        Variable [ Name=$sum ]
        DESC

      Limit
        LiteralExpr [LONG] [3]
    )
  )
]
]
FROM [  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [nation_key]
        :
        Variable [ Name=$nation_key ]
      )
      (
        LiteralExpr [STRING] [order_date]
        :
        Variable [ Name=$orderdate ]
      )
      (
        LiteralExpr [STRING] [sum_price]
        :
        FunctionCall asterix.sum@1[
          (
            SELECT ELEMENT [
            FieldAccessor [
              FieldAccessor [
                Variable [ Name=$g3 ]
                Field=orders
              ]
              Field=o_totalprice
            ]
            ]
            FROM [              Variable [ Name=$g3 ]
              AS Variable [ Name=$g3 ]
            ]
          )
        ]
      )
    ]
    ]
    FROM [      FunctionCall asterix.dataset@1[
        LiteralExpr [STRING] [tpch.Nation]
      ]
      AS Variable [ Name=$n ]
,
      FunctionCall asterix.dataset@1[
        LiteralExpr [STRING] [tpch.Customer]
      ]
      AS Variable [ Name=$customer ]
,
      FunctionCall asterix.dataset@1[
        LiteralExpr [STRING] [tpch.Orders]
      ]
      AS Variable [ Name=$orders ]
    ]
    Where
      OperatorExpr [
        OperatorExpr [
          FieldAccessor [
            Variable [ Name=$orders ]
            Field=o_custkey
          ]
          =
          FieldAccessor [
            Variable [ Name=$customer ]
            Field=c_custkey
          ]
        ]
        and
        OperatorExpr [
          FieldAccessor [
            Variable [ Name=$customer ]
            Field=c_nationkey
          ]
          =
          FieldAccessor [
            Variable [ Name=$n ]
            Field=n_nationkey
          ]
        ]
      ]
    Groupby
      Variable [ Name=$orderdate ]
      :=
      FieldAccessor [
        Variable [ Name=$orders ]
        Field=o_orderdate
      ]
      Variable [ Name=$nation_key ]
      :=
      FieldAccessor [
        Variable [ Name=$n ]
        Field=n_nationkey
      ]
      GROUP AS Variable [ Name=$g3 ]
      (
        n:=Variable [ Name=$n ]
        customer:=Variable [ Name=$customer ]
        orders:=Variable [ Name=$orders ]
      )

  )
  AS Variable [ Name=$x ]
]
Groupby
  Variable [ Name=$nation_key ]
  :=
  FieldAccessor [
    Variable [ Name=$x ]
    Field=nation_key
  ]
  GROUP AS Variable [ Name=$g ]
  (
    x:=Variable [ Name=$x ]
  )

Orderby
  Variable [ Name=$nation_key ]
  ASC

