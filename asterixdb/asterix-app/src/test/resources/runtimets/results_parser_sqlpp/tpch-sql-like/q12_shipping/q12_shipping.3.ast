DataverseUse tpch
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [l_shipmode]
    :
    Variable [ Name=$l_shipmode ]
  )
  (
    LiteralExpr [STRING] [high_line_count]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        CASE            OperatorExpr [
              OperatorExpr [
                FieldAccessor [
                  FieldAccessor [
                    Variable [ Name=$i ]
                    Field=o
                  ]
                  Field=o_orderpriority
                ]
                =
                LiteralExpr [STRING] [1-URGENT]
              ]
              or
              OperatorExpr [
                FieldAccessor [
                  FieldAccessor [
                    Variable [ Name=$i ]
                    Field=o
                  ]
                  Field=o_orderpriority
                ]
                =
                LiteralExpr [STRING] [2-HIGH]
              ]
            ]

        WHEN             LiteralExpr [TRUE]
        THEN             LiteralExpr [LONG] [1]

        WHEN             LiteralExpr [FALSE]
        THEN             LiteralExpr [LONG] [0]

        ELSE             LiteralExpr [NULL]

        END
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [low_line_count]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        CASE            OperatorExpr [
              OperatorExpr [
                FieldAccessor [
                  FieldAccessor [
                    Variable [ Name=$i ]
                    Field=o
                  ]
                  Field=o_orderpriority
                ]
                =
                LiteralExpr [STRING] [1-URGENT]
              ]
              or
              OperatorExpr [
                FieldAccessor [
                  FieldAccessor [
                    Variable [ Name=$i ]
                    Field=o
                  ]
                  Field=o_orderpriority
                ]
                =
                LiteralExpr [STRING] [2-HIGH]
              ]
            ]

        WHEN             LiteralExpr [TRUE]
        THEN             LiteralExpr [LONG] [0]

        WHEN             LiteralExpr [FALSE]
        THEN             LiteralExpr [LONG] [1]

        ELSE             LiteralExpr [NULL]

        END
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.LineItem]
  ]
  AS Variable [ Name=$l ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.Orders]
  ]
  AS Variable [ Name=$o ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$o ]
        Field=o_orderkey
      ]
      =
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_orderkey
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_commitdate
      ]
      <
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_receiptdate
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_shipdate
      ]
      <
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_commitdate
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_receiptdate
      ]
      >=
      LiteralExpr [STRING] [1994-01-01]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_receiptdate
      ]
      <
      LiteralExpr [STRING] [1995-01-01]
    ]
    and
    OperatorExpr [
      OperatorExpr [
        FieldAccessor [
          Variable [ Name=$l ]
          Field=l_shipmode
        ]
        =
        LiteralExpr [STRING] [MAIL]
      ]
      or
      OperatorExpr [
        FieldAccessor [
          Variable [ Name=$l ]
          Field=l_shipmode
        ]
        =
        LiteralExpr [STRING] [SHIP]
      ]
    ]
  ]
Groupby
  Variable [ Name=$l_shipmode ]
  :=
  FieldAccessor [
    Variable [ Name=$l ]
    Field=l_shipmode
  ]
  GROUP AS Variable [ Name=$g ]
  (
    l:=Variable [ Name=$l ]
    o:=Variable [ Name=$o ]
  )

Orderby
  Variable [ Name=$l_shipmode ]
  ASC

