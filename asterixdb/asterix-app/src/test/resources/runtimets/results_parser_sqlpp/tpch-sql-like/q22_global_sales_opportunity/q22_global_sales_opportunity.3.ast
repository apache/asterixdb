DataverseUse tpch
FunctionDecl q22_customer_tmp([]) {
  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [c_acctbal]
        :
        FieldAccessor [
          Variable [ Name=$c ]
          Field=c_acctbal
        ]
      )
      (
        LiteralExpr [STRING] [c_custkey]
        :
        FieldAccessor [
          Variable [ Name=$c ]
          Field=c_custkey
        ]
      )
      (
        LiteralExpr [STRING] [cntrycode]
        :
        FunctionCall tpch.SUBSTR@3[
          FieldAccessor [
            Variable [ Name=$c ]
            Field=c_phone
          ]
          LiteralExpr [LONG] [0]
          LiteralExpr [LONG] [2]
        ]
      )
    ]
    ]
    FROM [      Variable [ Name=$Customer ]
      AS Variable [ Name=$c ]
    ]
  )
}

Query:
Let Variable [ Name=$avg ]
  :=
  FunctionCall asterix.avg@1[
    (
      SELECT ELEMENT [
      FieldAccessor [
        Variable [ Name=$c ]
        Field=c_acctbal
      ]
      ]
      FROM [        FunctionCall asterix.dataset@1[
          LiteralExpr [STRING] [tpch.Customer]
        ]
        AS Variable [ Name=$c ]
      ]
      Where
        OperatorExpr [
          FieldAccessor [
            Variable [ Name=$c ]
            Field=c_acctbal
          ]
          >
          LiteralExpr [DOUBLE] [0.0]
        ]
    )
  ]
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [cntrycode]
    :
    Variable [ Name=$cntrycode ]
  )
  (
    LiteralExpr [STRING] [numcust]
    :
    FunctionCall asterix.count@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          Variable [ Name=$g ]
          Field=ct
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$g ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [totacctbal]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          Variable [ Name=$i ]
          Field=c_acctbal
        ]
        ]
        FROM [          (
            SELECT ELEMENT [
            FieldAccessor [
              Variable [ Name=$g ]
              Field=ct
            ]
            ]
            FROM [              Variable [ Name=$g ]
              AS Variable [ Name=$g ]
            ]
          )
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
]
]
FROM [  FunctionCall tpch.q22_customer_tmp@0[
  ]
  AS Variable [ Name=$ct ]
]
Where
  OperatorExpr [
    FieldAccessor [
      Variable [ Name=$ct ]
      Field=c_acctbal
    ]
    >
    Variable [ Name=$avg ]
  ]
Groupby
  Variable [ Name=$cntrycode ]
  :=
  FieldAccessor [
    Variable [ Name=$ct ]
    Field=cntrycode
  ]
  GROUP AS Variable [ Name=$g ]
  (
    ct:=Variable [ Name=$ct ]
  )

Orderby
  Variable [ Name=$cntrycode ]
  ASC

