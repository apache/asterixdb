DataverseUse tpch
Query:
SELECT ELEMENT [
OperatorExpr [
  LiteralExpr [DOUBLE] [100.0]
  *
  FunctionCall asterix.sum@1[
    (
      SELECT ELEMENT [
      CASE          OperatorExpr [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=p_type
            ]
            like
            LiteralExpr [STRING] [PROMO%]
          ]

      WHEN           LiteralExpr [TRUE]
      THEN           OperatorExpr [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=l_extendedprice
            ]
            *
            OperatorExpr [
              Variable [ Name=$t ]
              -
              FieldAccessor [
                Variable [ Name=$i ]
                Field=l_discount
              ]
            ]
          ]

      ELSE           LiteralExpr [DOUBLE] [0.0]

      END
      ]
      FROM [        (
          SELECT ELEMENT [
          FieldAccessor [
            Variable [ Name=$g ]
            Field=lp
          ]
          ]
          FROM [            Variable [ Name=$g ]
            AS Variable [ Name=$g ]
          ]
        )
        AS Variable [ Name=$i ]
      ]
    )
  ]
  /
  FunctionCall asterix.sum@1[
    (
      SELECT ELEMENT [
      OperatorExpr [
        FieldAccessor [
          Variable [ Name=$i ]
          Field=l_extendedprice
        ]
        *
        OperatorExpr [
          Variable [ Name=$t ]
          -
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l_discount
          ]
        ]
      ]
      ]
      FROM [        (
          SELECT ELEMENT [
          FieldAccessor [
            Variable [ Name=$g ]
            Field=lp
          ]
          ]
          FROM [            Variable [ Name=$g ]
            AS Variable [ Name=$g ]
          ]
        )
        AS Variable [ Name=$i ]
      ]
    )
  ]
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.LineItem]
  ]
  AS Variable [ Name=$l ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.Part]
  ]
  AS Variable [ Name=$p ]
]
Let Variable [ Name=$lp ]
  :=
  RecordConstructor [
    (
      LiteralExpr [STRING] [p_type]
      :
      FieldAccessor [
        Variable [ Name=$p ]
        Field=p_type
      ]
    )
    (
      LiteralExpr [STRING] [l_extendedprice]
      :
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_extendedprice
      ]
    )
    (
      LiteralExpr [STRING] [l_discount]
      :
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_discount
      ]
    )
  ]
Where
  OperatorExpr [
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_partkey
      ]
      =
      FieldAccessor [
        Variable [ Name=$p ]
        Field=p_partkey
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_shipdate
      ]
      >=
      LiteralExpr [STRING] [1995-09-01]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$l ]
        Field=l_shipdate
      ]
      <
      LiteralExpr [STRING] [1995-10-01]
    ]
  ]
Groupby
  Variable [ Name=$t ]
  :=
  LiteralExpr [LONG] [1]
  GROUP AS Variable [ Name=$g ]
  (
    lp:=Variable [ Name=$lp ]
  )

