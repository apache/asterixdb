DataverseUse test
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [id]
    :
    Variable [ Name=$id ]
  )
  (
    LiteralExpr [STRING] [length]
    :
    FunctionCall asterix.string-length@1[
      Variable [ Name=$comment ]
    ]
  )
  (
    LiteralExpr [STRING] [comment]
    :
    Variable [ Name=$comment ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.Line]
  ]
  AS Variable [ Name=$i ]
]
Groupby
  Variable [ Name=$comment ]
  :=
  FieldAccessor [
    Variable [ Name=$i ]
    Field=l_comment
  ]
  Variable [ Name=$id ]
  :=
  FieldAccessor [
    Variable [ Name=$i ]
    Field=l_orderkey
  ]
  GROUP AS Variable [ Name=#1 ]
  (
    i:=Variable [ Name=$i ]
  )

Orderby
  Variable [ Name=$id ]
  ASC
  FunctionCall asterix.string-length@1[
    Variable [ Name=$comment ]
  ]
  ASC
  Variable [ Name=$comment ]
  ASC

