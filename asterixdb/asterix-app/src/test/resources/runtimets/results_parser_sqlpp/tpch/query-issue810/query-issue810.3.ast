DataverseUse tpch
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [l_returnflag]
    :
    Variable [ Name=$l_returnflag ]
  )
  (
    LiteralExpr [STRING] [l_linestatus]
    :
    Variable [ Name=$l_linestatus ]
  )
  (
    LiteralExpr [STRING] [count_cheaps]
    :
    FunctionCall asterix.count@1[
      Variable [ Name=$cheap ]
    ]
  )
  (
    LiteralExpr [STRING] [count_expensives]
    :
    FunctionCall asterix.count@1[
      Variable [ Name=$expensive ]
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.LineItem]
  ]
  AS Variable [ Name=$l ]
]
Where
  OperatorExpr [
    FieldAccessor [
      Variable [ Name=$l ]
      Field=l_shipdate
    ]
    <=
    LiteralExpr [STRING] [1998-09-02]
  ]
Groupby
  Variable [ Name=$l_returnflag ]
  :=
  FieldAccessor [
    Variable [ Name=$l ]
    Field=l_returnflag
  ]
  Variable [ Name=$l_linestatus ]
  :=
  FieldAccessor [
    Variable [ Name=$l ]
    Field=l_linestatus
  ]
  GROUP AS Variable [ Name=$g ]
  (
    l:=Variable [ Name=$l ]
  )

Let Variable [ Name=$cheap ]
  :=
  (
    SELECT ELEMENT [
    FieldAccessor [
      Variable [ Name=$m ]
      Field=l
    ]
    ]
    FROM [      Variable [ Name=$g ]
      AS Variable [ Name=$m ]
    ]
    Where
      OperatorExpr [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$m ]
            Field=l
          ]
          Field=l_discount
        ]
        >
        LiteralExpr [DOUBLE] [0.05]
      ]
  )
Let Variable [ Name=$expensive ]
  :=
  (
    SELECT ELEMENT [
    FieldAccessor [
      Variable [ Name=$a ]
      Field=l
    ]
    ]
    FROM [      Variable [ Name=$g ]
      AS Variable [ Name=$a ]
    ]
    Where
      OperatorExpr [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$a ]
            Field=l
          ]
          Field=l_discount
        ]
        <=
        LiteralExpr [DOUBLE] [0.05]
      ]
  )
Orderby
  Variable [ Name=$l_returnflag ]
  ASC
  Variable [ Name=$l_linestatus ]
  ASC

