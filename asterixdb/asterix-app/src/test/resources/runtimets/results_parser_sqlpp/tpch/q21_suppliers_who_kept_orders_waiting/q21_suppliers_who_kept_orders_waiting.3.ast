                SELECT ELEMENT [
                FieldAccessor [
                  Variable [ Name=$g2 ]
                  Field=l2
                ]
                ]
                FROM [                  Variable [ Name=$g2 ]
                  AS Variable [ Name=$g2 ]
                ]
              )
              AS Variable [ Name=$i ]
            ]
          )
        ]
      )
      (
        LiteralExpr [STRING] [max_suppkey]
