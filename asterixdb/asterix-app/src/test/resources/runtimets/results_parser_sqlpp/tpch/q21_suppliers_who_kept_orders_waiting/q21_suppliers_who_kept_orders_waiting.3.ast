DataverseUse tpch
FunctionDecl tmp1([]) {
  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [l_orderkey]
        :
        Variable [ Name=$l_orderkey ]
      )
      (
        LiteralExpr [STRING] [count_suppkey]
        :
        FunctionCall asterix.count@1[
          (
            SELECT ELEMENT [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=l_suppkey
            ]
            ]
            FROM [              (
                SELECT ELEMENT [
                FieldAccessor [
                  Variable [ Name=$g1 ]
                  Field=l2
                ]
                ]
                FROM [                  Variable [ Name=$g1 ]
                  AS Variable [ Name=$g1 ]
                ]
              )
              AS Variable [ Name=$i ]
            ]
          )
        ]
      )
      (
        LiteralExpr [STRING] [max_suppkey]
        :
        FunctionCall asterix.max@1[
          (
            SELECT ELEMENT [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=l_suppkey
            ]
            ]
            FROM [              (
                SELECT ELEMENT [
                FieldAccessor [
                  Variable [ Name=$g1 ]
                  Field=l2
                ]
                ]
                FROM [                  Variable [ Name=$g1 ]
                  AS Variable [ Name=$g1 ]
                ]
              )
              AS Variable [ Name=$i ]
            ]
          )
        ]
      )
    ]
    ]
    FROM [      (
        SELECT ELEMENT [
        RecordConstructor [
          (
            LiteralExpr [STRING] [l_orderkey]
            :
            Variable [ Name=$l_orderkey1 ]
          )
          (
            LiteralExpr [STRING] [l_suppkey]
            :
            Variable [ Name=$l_suppkey1 ]
          )
        ]
        ]
        FROM [          Variable [ Name=$LineItem ]
          AS Variable [ Name=$l ]
        ]
        Groupby
          Variable [ Name=$l_orderkey1 ]
          :=
          FieldAccessor [
            Variable [ Name=$l ]
            Field=l_orderkey
          ]
          Variable [ Name=$l_suppkey1 ]
          :=
          FieldAccessor [
            Variable [ Name=$l ]
            Field=l_suppkey
          ]

      )
      AS Variable [ Name=$l2 ]
    ]
    Groupby
      Variable [ Name=$l_orderkey ]
      :=
      FieldAccessor [
        Variable [ Name=$l2 ]
        Field=l_orderkey
      ]
      GROUP AS Variable [ Name=$g1 ]

  )
}

FunctionDecl tmp2([]) {
  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [l_orderkey]
        :
        Variable [ Name=$l_orderkey ]
      )
      (
        LiteralExpr [STRING] [count_suppkey]
        :
        FunctionCall asterix.count@1[
          (
            SELECT ELEMENT [
            FieldAccessor [
              Variable [ Name=$i ]
