use dataverse test;

load dataset HRM_Raw using localfs
    (("path"="asterix_nc1://data/csv/55.csv"),
    ("format"="delimited-text"),
    ("header"="true"));

insert into dataset HRM
(
    for $t in dataset HRM_Raw
    return {
      "row_id": $t.row_id,
      "sid" : $t.sid,
      "date" : parse-date($t.date, "YMD"),
      "time" : parse-time($t.time, "h:m:s"),
      "day" : $t.day,
      "bpm" : $t.bpm,
      "RR" : [$t.RR]
})