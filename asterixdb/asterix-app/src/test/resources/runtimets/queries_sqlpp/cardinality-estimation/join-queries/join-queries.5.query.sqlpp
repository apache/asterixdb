USE tpch;

EXPLAIN
SELECT COUNT(*)
FROM LineItem l, Orders o, Customer c
WHERE l.l_orderkey = o.o_orderkey
  AND o.o_custkey = c.c_custkey
GROUP BY l.l_partkey, o.o_orderstatus, c.c_nationkey;
