/*
 * Description     : Test that the following statement is prohibited in readonly request mode:
 *                 : "upsert"
 * Expected Result : Failure
 */

-- param readonly:string=true

use test;

upsert into t2
([
  {"id":1, "c1":100},
  {"id":2, "c1":200},
  {"id":3, "c1":300}
]);
