/*
 * Description  : Isomorphic Subplan functions Test
 * Expected Res : SUCCESS
 */
use test;


SELECT l.timestamp, (l.`value` + r.`value`) as `value`
FROM
    ( SELECT timestamp, `value` FROM stock_market_price_usd sub  WHERE ANY x in sub.arr satisfies x = 0 ) l
        JOIN
    ( SELECT timestamp, `value` FROM stock_market_price_usd sub  WHERE ANY x in sub.arr satisfies x = 0 ) r
    ON l.timestamp = r.timestamp
order by timestamp;

