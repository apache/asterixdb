/*
 * Description      :   sql-strict_sum() aggregate function must return the numeric array_sum, when non null values are given as input to sql-strict_sum().
 *                  :   Get the array_sum for those tuples which are non null for salary fields.
 * Expected result  :   Success
 * Date             :   July 20th 2012
 */

use test;


insert into tdst
select element {'id':123,'sal':1000};
insert into tdst
select element {'id':113,'sal':2000};
insert into tdst
select element {'id':163,'sal':3000};
insert into tdst
select element {'id':161,'sal':4000};
insert into tdst
select element {'id':173,'sal':5000};
insert into tdst
select element {'id':183,'sal':null};
