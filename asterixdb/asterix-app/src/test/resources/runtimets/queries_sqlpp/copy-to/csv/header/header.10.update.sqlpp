/*
 * Default Null Test
 */

USE test;

COPY (
   SELECT id, null name, amount, accountNumber FROM TestCollection
) toWriter
TO %adapter%
PATH (%pathprefix% "copy-to-result", "csv", "default", "null")
AS (id bigint, name STRING, amount float, accountNumber double)
WITH {
    %template_colons%,
    %additionalProperties%
    "format":"csv",
    "delimiter":"|",
    "header":"true"
}