SELECT
  CASE
    WHEN t = 0 THEN [0]
    WHEN t IS NULL THEN (SELECT -1)
    WHEN t IS MISSING THEN (SELECT -2)
    WHEN t =1 THEN NULL
    WHEN t=2 THEN MISSING
    ELSE 2.0/t
  END
FROM [0, 1, 2, 4, NULL, [0][-1]] t;

