drop dataverse test if exists;
create dataverse test;
use test;

create type t1 as {
id : int,
str1 : string,
d1 : date,
t1 : time,
dt1 : datetime,
dur1 : duration,
ymd1 : `year_month_duration`,
dtd1 : `day_time_duration`
};

create dataset ds1(t1) primary key id;