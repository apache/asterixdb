-- compareunorderedarray=true
use test;

SELECT f1,
COUNT(DISTINCT test11) AS f2,
COUNT(DISTINCT test12) as f3,
COUNT((SELECT m FROM el)) AS f4
FROM test1
LET f1 = (CASE WHEN (ANY a IN test1.states SATISFIES a = 1) THEN "1" WHEN (ANY a IN test1.states SATISFIES a = sleep(2,100)) THEN "2" ELSE "3" END),
el = (SELECT s,m FROM test1.elements)
GROUP BY f1;