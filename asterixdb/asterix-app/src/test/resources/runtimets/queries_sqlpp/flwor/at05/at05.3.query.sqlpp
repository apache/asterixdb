/*
 * Description      :  Test for clause of the position variable in FLWOR expression using tpch
 * Expected Result  :  Success
 * Date             :  07/18/2013
 */

use test;


select element si
from
    ( select element {'xx':xx,'x': (from g select value g.x),'uid':uid}
      from  Orders as x
      where x.o_custkey < 4
      group by x.o_custkey as uid
      group as g
      let  xx = (
            select element {'uid':uid,'seq':i,'item':y.o_orderkey}
            from  (
                select element xxx.x
                from  g as xxx
                order by xxx.x.o_orderkey
            ) as y at i
        )
      order by uid
    ) as gen0,
    gen0.xx as si
;
