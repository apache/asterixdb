/*
 * Description  : Try to drop a function dependency.
 *                Non-varargs function uses varargs function.
 * Expected Res : Error
 */

drop dataverse B if exists;
drop dataverse C if exists;
create dataverse B;
create dataverse C;

use C;

create function f2(...) {
  contains(args[0], args[1])
};

use B;

create function f3(message, text) {
  C.f2(message, text)
};

drop dataverse C;