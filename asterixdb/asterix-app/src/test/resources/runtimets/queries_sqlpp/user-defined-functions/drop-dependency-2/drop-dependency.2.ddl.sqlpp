  tweetid: uuid,
  sender_location: point,
  send_time: datetime,
  referred_topics: {{ string }},
  message_text: string,
  countA: int32,
  countB: int32
};

create dataset TweetMessages(TweetMessageType)
primary key tweetid autogenerated;

create function f1(message, text){
  contains(message,text)
};

