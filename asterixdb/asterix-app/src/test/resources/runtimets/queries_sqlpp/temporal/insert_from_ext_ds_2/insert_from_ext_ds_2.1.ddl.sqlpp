/*
 * Test case name: insert_from_ext_ds_2
 * Description: verify external data loading on temporal types
 * Expected result: success
 */

drop  dataverse timeTest if exists;
create  dataverse timeTest;

use timeTest;


create type timeTest.timesType as
{
  date : date,
  time : time,
  datetime : datetime,
  duration : duration,
  `year-month-duration` : `year_month_duration`,
  `day-time-duration` : `day_time_duration`,
  `date-interval` : interval,
  `time-interval` : interval,
  `datetime-interval` : interval
};

create  dataset timeData(timesType) primary key date;

