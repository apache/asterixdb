use test;

/*
 * use-index hint. use specified indexes (idx_1k_2k, idx_2k_5k)
 * explain plan -> intersection between idx_1k_2k and idx_2k_5k
 */

select value unique1
from tenk
where
  thousand /* +use-index(idx_1k_2k) */ = 0
and
  twothousand /* +use-index(idx_2k_5k) */ = 0
order by unique1;