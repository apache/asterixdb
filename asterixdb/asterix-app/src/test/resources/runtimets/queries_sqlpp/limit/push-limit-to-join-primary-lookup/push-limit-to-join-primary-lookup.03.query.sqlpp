USE test;
SELECT COUNT(*) FROM (
SELECT
    ds1.field1,
    ds1.field2,
    ds1.field3,
    ds1.field4,
    comp_field,
    ds2.fieldX AS fieldX
FROM ds1 AS ds1
JOIN ds2 AS ds2
    ON ds2.id /*+ indexnl */ = (ds1.field2 || ":" || TO_STRING(ds1.field3) || ":" || ds1.field4)
    LET comp_field = (REPLACE(ds1.field1, "-", "") || "03")
LIMIT 2) sub
;