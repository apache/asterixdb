USE test;

EXPLAIN
SELECT COUNT(*)
FROM orders o1, orders o2
WHERE o1.o_carrier_id = o2.o_carrier_id
  -- Both predicates should be pushed and combined as a disjunctive predicate
  AND o1.o_ol_cnt > 4
  AND o2.o_ol_cnt = 1