SET `compiler.sort.parallel` "false";
USE test;

SELECT DISTINCT t.user_id FROM (
SELECT ds1.id AS user_id FROM ds1 WHERE ds1.val >= 30 AND ds1.val <= 50
UNION ALL
SELECT ds1.id AS user_id FROM ds_disjoint AS c JOIN ds1 ON c.id /*+ indexnl */ = ds1.id
) AS t
ORDER BY t.user_id;