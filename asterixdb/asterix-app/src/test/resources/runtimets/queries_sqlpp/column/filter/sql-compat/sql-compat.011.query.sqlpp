// requesttype=application/json
// param sql-compat:json=true

USE test;

SET `compiler.column.filter` "false";



SELECT v.str2 AS str2,
       SUM(v.num3) AS `sum:num3:ok`
FROM ColumnView v
WHERE ((v.str2 IN ('sixteen')) OR (v.str2 IS UNKNOWN))
GROUP BY v.str2
ORDER BY v.str2;
