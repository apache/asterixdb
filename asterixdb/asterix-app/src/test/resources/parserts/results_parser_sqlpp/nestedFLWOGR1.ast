Query:
SELECT ELEMENT [
(
  SELECT ELEMENT [
  Variable [ Name=$k ]
  ]
  FROM [    CASE        OperatorExpr [
          Variable [ Name=$i ]
          >
          Variable [ Name=$j ]
        ]

    WHEN         LiteralExpr [TRUE]
    THEN         Variable [ Name=$i ]

    ELSE         Variable [ Name=$j ]

    END
    AS Variable [ Name=$k ]
  ]
  Where
    OperatorExpr [
      Variable [ Name=$k ]
      <
      LiteralExpr [LONG] [10]
    ]
)
]
FROM [  OrderedListConstructor [
    LiteralExpr [LONG] [1]
    LiteralExpr [LONG] [2]
    LiteralExpr [LONG] [30]
    LiteralExpr [LONG] [40]
  ]
  AS Variable [ Name=$i ]
,
  UnorderedListConstructor [
    LiteralExpr [LONG] [4]
    LiteralExpr [LONG] [5]
    LiteralExpr [LONG] [6]
  ]
  AS Variable [ Name=$j ]
]
