Query:
SELECT [
FunctionCall asterix.sql-sum@1[
  (
    SELECT ELEMENT [
    OperatorExpr [
      FieldAccessor [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=#4 ]
            Field=root
          ]
          Field=t
        ]
        Field=a
      ]
      *
      FieldAccessor [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=#4 ]
            Field=root
          ]
          Field=t
        ]
        Field=b
      ]
    ]
    ]
    FROM [      Variable [ Name=#1 ]
      AS Variable [ Name=#4 ]
    ]
  )
]
root
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tbl_name]
  ]
  AS Variable [ Name=$root ]
]
Groupby
  Variable [ Name=$id ]
  :=
  FieldAccessor [
    Variable [ Name=$root ]
    Field=id
  ]
  GROUP AS Variable [ Name=#1 ]
  (
    root:=Variable [ Name=$root ]
  )

Let Variable [ Name=$u ]
  :=
  FunctionCall asterix.sql-min@1[
    (
      SELECT ELEMENT [
      FieldAccessor [
        FieldAccessor [
          Variable [ Name=#2 ]
          Field=root
        ]
        Field=time
      ]
      ]
      FROM [        Variable [ Name=#1 ]
        AS Variable [ Name=#2 ]
      ]
    )
  ]
 HAVING
  OperatorExpr [
    FunctionCall asterix.sql-count@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=#3 ]
            Field=root
          ]
          Field=orders
        ]
        ]
        FROM [          Variable [ Name=#1 ]
          AS Variable [ Name=#3 ]
        ]
      )
    ]
    >
    LiteralExpr [LONG] [0]
  ]
Orderby
  Variable [ Name=$u ]
  ASC

