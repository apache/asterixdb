Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [user_name]
    :
    FieldAccessor [
      Variable [ Name=$user ]
      Field=name
    ]
  )
  (
    LiteralExpr [STRING] [similar_users]
    :
    Variable [ Name=$similar_users ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [Default.Users]
  ]
  AS Variable [ Name=$user ]
]
Let Variable [ Name=$similar_users ]
  :=
  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [user_name]
        :
        FieldAccessor [
          Variable [ Name=$similar_user ]
          Field=name
        ]
      )
      (
        LiteralExpr [STRING] [similarity]
        :
        Variable [ Name=$similarity ]
      )
    ]
    ]
    FROM [      FunctionCall asterix.dataset@1[
        LiteralExpr [STRING] [Default.Users]
      ]
      AS Variable [ Name=$similar_user ]
    ]
    Let Variable [ Name=$similarity ]
      :=
      FunctionCall asterix.similarity-jaccard@2[
        FieldAccessor [
          Variable [ Name=$user ]
          Field=interests
        ]
        FieldAccessor [
          Variable [ Name=$similar_user ]
          Field=interests
        ]
      ]
    Where
      OperatorExpr [
        OperatorExpr [
          Variable [ Name=$user ]
          !=
          Variable [ Name=$similar_user ]
        ]
        and
        OperatorExpr [
          Variable [ Name=$similarity ]
          >=
          LiteralExpr [DOUBLE] [0.75]
        ]
      ]
    Orderby
      Variable [ Name=$similarity ]
      DESC

    Limit
      LiteralExpr [LONG] [10]
  )
