Query:
SELECT ELEMENT [
FunctionCall asterix.switch-case[
  LiteralExpr [TRUE]
  OperatorExpr [
    Variable [ Name=$i ]
    >
    Variable [ Name=$j ]
  ]
  Variable [ Name=$i ]
  Variable [ Name=$j ]
]
]
FROM [  OrderedListConstructor [
    LiteralExpr [LONG] [1]
    LiteralExpr [LONG] [2]
    LiteralExpr [LONG] [30]
    LiteralExpr [LONG] [40]
  ]
  AS Variable [ Name=$i ]
,
  UnorderedListConstructor [
    LiteralExpr [LONG] [4]
    LiteralExpr [LONG] [5]
    LiteralExpr [LONG] [6]
  ]
  AS Variable [ Name=$j ]
]
