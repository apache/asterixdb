DataverseUse test
WriteOutputTo asterix_nc1:rttest/filter-nested.adm
TypeDecl TestTypetmp [
  open RecordType {
    fname : string,
    lname : string
  }
]
TypeDecl TestType [
  open RecordType {
    nested : TestTypetmp
  }
]
DatasetDecl testdst(TestType) partitioned by [[nested, fname]]
Query:
SELECT ELEMENT [
FieldAccessor [
  Variable [ Name=$emp ]
  Field=nested
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.testdst]
  ]
  AS Variable [ Name=$emp ]
]
Where
  OperatorExpr [
    FieldAccessor [
      FieldAccessor [
        Variable [ Name=$emp ]
        Field=nested
      ]
      Field=lname
    ]
    >
    LiteralExpr [STRING] [Roger]
  ]
