DataverseUse test
TypeDecl TestType [
  closed RecordType {
    key1 : integer,
    value : integer
  }
]
DatasetDecl test(TestType) partitioned by [[key1]]
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [gid]
    :
    Variable [ Name=$aid ]
  )
  (
    LiteralExpr [STRING] [avg]
    :
    FunctionCall test.avg@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          Variable [ Name=$j ]
          Field=value
        ]
        ]
        FROM [          FunctionCall Metadata.dataset@1[
            LiteralExpr [STRING] [i]
          ]
          AS Variable [ Name=$j ]
        ]
        Where
          FunctionCall null.not@1[
            FunctionCall null.is-null@1[
              FieldAccessor [
                Variable [ Name=$j ]
                Field=value
              ]
            ]
          ]
      )
    ]
  )
]
]
FROM [  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [test]
  ]
  AS Variable [ Name=$i ]
]
Groupby
  Variable [ Name=$aid ]
  :=
  FieldAccessor [
    Variable [ Name=$i ]
    Field=key1
  ]
  GROUP AS Variable [ Name=#1 ]
  (
    i:=Variable [ Name=$i ]
  )

