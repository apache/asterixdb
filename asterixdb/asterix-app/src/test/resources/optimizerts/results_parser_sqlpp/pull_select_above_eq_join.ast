DataverseUse pull-select-above-eq-join
TypeDecl UserType [
  open RecordType {
    uid : integer,
    name : string,
    city : string,
    lottery_numbers : UnorderedList <integer>

  }
]
TypeDecl VisitorType [
  open RecordType {
    vid : integer,
    name : string,
    city : string,
    lottery_numbers : UnorderedList <integer>

  }
]
DatasetDecl Users(UserType) partitioned by [[uid]]
DatasetDecl Visitors(VisitorType) partitioned by [[vid]]
WriteOutputTo asterix_nc1:/tmp/pull-select-above-eq-join.adm
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [uid]
    :
    FieldAccessor [
      Variable [ Name=$user ]
      Field=uid
    ]
  )
  (
    LiteralExpr [STRING] [vid]
    :
    FieldAccessor [
      Variable [ Name=$visitor ]
      Field=vid
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [pull-select-above-eq-join.Users]
  ]
  AS Variable [ Name=$user ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [pull-select-above-eq-join.Visitors]
  ]
  AS Variable [ Name=$visitor ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$user ]
        Field=name
      ]
      =
      FieldAccessor [
        Variable [ Name=$visitor ]
        Field=name
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$user ]
        Field=city
      ]
      !=
      FieldAccessor [
        Variable [ Name=$visitor ]
        Field=city
      ]
    ]
  ]
