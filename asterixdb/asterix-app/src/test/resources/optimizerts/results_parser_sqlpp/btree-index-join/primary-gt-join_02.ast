TypeDecl TestType [
  open RecordType {
    key1 : integer,
    key2 : integer,
    fname : string,
    lname : string
  }
]
DatasetDecl DsOne(TestType) partitioned by [[key1]]
DatasetDecl DsTwo(TestType) partitioned by [[key1]]
WriteOutputTo asterix_nc1:rttest/btree-index-join_primary-gt-join_02.adm
Query:
SELECT ELEMENT [
Variable [ Name=$x ]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test1.DsOne]
  ]
  AS Variable [ Name=$x ]
,
  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test1.DsTwo]
  ]
  AS Variable [ Name=$y ]
]
Where
  OperatorExpr [
    FieldAccessor [
      Variable [ Name=$x ]
      Field=key2
    ]
    <
    FieldAccessor [
      Variable [ Name=$y ]
      Field=key1
    ]
  ]
