DataverseUse tpch
TypeDecl LineItemType [
  closed RecordType {
    l_orderkey : integer,
    l_partkey : integer,
    l_suppkey : integer,
    l_linenumber : integer,
    l_quantity : double,
    l_extendedprice : double,
    l_discount : double,
    l_tax : double,
    l_returnflag : string,
    l_linestatus : string,
    l_shipdate : string,
    l_commitdate : string,
    l_receiptdate : string,
    l_shipinstruct : string,
    l_shipmode : string,
    l_comment : string
  }
]
DatasetDecl LineItem(LineItemType) partitioned by [[l_orderkey], [l_linenumber]]
WriteOutputTo asterix_nc1:rttest/tpch_q1_pricing_summary_report_nt.adm
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [l_returnflag]
    :
    Variable [ Name=$l_returnflag ]
  )
  (
    LiteralExpr [STRING] [l_linestatus]
    :
    Variable [ Name=$l_linestatus ]
  )
  (
    LiteralExpr [STRING] [sum_qty]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l
          ]
          Field=l_quantity
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [sum_base_price]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l
          ]
          Field=l_extendedprice
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [sum_disc_price]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        OperatorExpr [
          FieldAccessor [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=l
            ]
            Field=l_extendedprice
          ]
          *
          OperatorExpr [
            LiteralExpr [LONG] [1]
            -
            FieldAccessor [
              FieldAccessor [
                Variable [ Name=$i ]
                Field=l
              ]
              Field=l_discount
            ]
          ]
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [sum_charge]
    :
    FunctionCall asterix.sum@1[
      (
        SELECT ELEMENT [
        OperatorExpr [
          FieldAccessor [
            FieldAccessor [
              Variable [ Name=$i ]
              Field=l
            ]
            Field=l_extendedprice
          ]
          *
          OperatorExpr [
            LiteralExpr [LONG] [1]
            -
            FieldAccessor [
              FieldAccessor [
                Variable [ Name=$i ]
                Field=l
              ]
              Field=l_discount
            ]
          ]
          *
          OperatorExpr [
            LiteralExpr [LONG] [1]
            +
            FieldAccessor [
              FieldAccessor [
                Variable [ Name=$i ]
                Field=l
              ]
              Field=l_tax
            ]
          ]
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [ave_qty]
    :
    FunctionCall asterix.avg@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l
          ]
          Field=l_quantity
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [ave_price]
    :
    FunctionCall asterix.avg@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l
          ]
          Field=l_extendedprice
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [ave_disc]
    :
    FunctionCall asterix.avg@1[
      (
        SELECT ELEMENT [
        FieldAccessor [
          FieldAccessor [
            Variable [ Name=$i ]
            Field=l
          ]
          Field=l_discount
        ]
        ]
        FROM [          Variable [ Name=$g ]
          AS Variable [ Name=$i ]
        ]
      )
    ]
  )
  (
    LiteralExpr [STRING] [count_order]
    :
    FunctionCall asterix.count@1[
      Variable [ Name=$g ]
    ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [tpch.LineItem]
  ]
  AS Variable [ Name=$l ]
]
Where
  OperatorExpr [
    FieldAccessor [
      Variable [ Name=$l ]
      Field=l_shipdate
    ]
    <=
    LiteralExpr [STRING] [1998-09-02]
  ]
Groupby
  Variable [ Name=$l_returnflag ]
  :=
  FieldAccessor [
    Variable [ Name=$l ]
    Field=l_returnflag
  ]
  Variable [ Name=$l_linestatus ]
  :=
  FieldAccessor [
    Variable [ Name=$l ]
    Field=l_linestatus
  ]
  GROUP AS Variable [ Name=$g ]
  (
    l:=Variable [ Name=$l ]
  )

Orderby
  Variable [ Name=$l_returnflag ]
  ASC
  Variable [ Name=$l_linestatus ]
  ASC

