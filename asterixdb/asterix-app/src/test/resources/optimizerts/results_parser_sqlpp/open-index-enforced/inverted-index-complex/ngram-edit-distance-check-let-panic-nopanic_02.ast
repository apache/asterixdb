DataverseUse test
TypeDecl DBLPType [
  open RecordType {
    id : integer,
    dblpid : string,
    title : string,
    misc : string
  }
]
DatasetDecl DBLP(DBLPType) partitioned by [[id]]
WriteOutputTo asterix_nc1:rttest/inverted-index-complex_ngram-edit-distance-check-let-panic-nopanic_01.adm
Query:
SELECT ELEMENT [
Variable [ Name=$o ]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.DBLP]
  ]
  AS Variable [ Name=$o ]
]
Let Variable [ Name=$edb ]
  :=
  FunctionCall asterix.edit-distance-check@3[
    FieldAccessor [
      Variable [ Name=$o ]
      Field=authors
    ]
    LiteralExpr [STRING] [Amihay Motro]
    LiteralExpr [LONG] [5]
  ]
Let Variable [ Name=$eda ]
  :=
  FunctionCall asterix.edit-distance-check@3[
    FieldAccessor [
      Variable [ Name=$o ]
      Field=authors
    ]
    LiteralExpr [STRING] [Amihay Motro]
    LiteralExpr [LONG] [3]
  ]
Where
  OperatorExpr [
    IndexAccessor [
      Variable [ Name=$edb ]
      Index:       LiteralExpr [LONG] [0]
    ]
    and
    IndexAccessor [
      Variable [ Name=$eda ]
      Index:       LiteralExpr [LONG] [0]
    ]
  ]
