                exchange
                -- HASH_PARTITION_EXCHANGE [$$20]  |PARTITIONED|
                  project ([$$23, $$20])
                  -- STREAM_PROJECT  |PARTITIONED|
                    exchange
                    -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
                      left outer join (eq($$21, $#1))
                      -- HYBRID_HASH_JOIN [$$21][$#1]  |PARTITIONED|
                        exchange
