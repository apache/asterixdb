distribute result [$$o]
-- DISTRIBUTE_RESULT  |PARTITIONED|
  exchange
  -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
    project ([$$o])
    -- STREAM_PROJECT  |PARTITIONED|
      exchange
      -- ONE_TO_ONE_EXCHANGE  |PARTITIONED|
        order (ASC, $$17)
        -- STABLE_SORT [$$17(ASC)]  |PARTITIONED|
          exchange
          -- RANGE_PARTITION_EXCHANGE [$$17(ASC)]  |PARTITIONED|
            forward: shared-variable = $$25
