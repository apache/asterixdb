DROP DATAVERSE test IF EXISTS;
CREATE DATAVERSE test;

USE test;

CREATE TYPE OpenType AS {
    uid: uuid
};

CREATE DATASET OpenDataset(OpenType)
PRIMARY KEY uid AUTOGENERATED;

SET `compiler.sort.parallel` "true";
COPY OpenDataset x
TO localfs
PATH("myPath")
OVER(ORDER BY x.id DESC)
WITH {
   "format": "json"
};
