// testing select-quantified with JOIN on other fields and making sure the query does not fail.
// the array index is currently not used due to the JOIN.
SET `compiler.arrayindex` "true";

DROP DATAVERSE TestYelp IF EXISTS;
CREATE DATAVERSE TestYelp;
USE TestYelp;

CREATE TYPE OpenType AS {
    id: int
};

CREATE DATASET sales(OpenType) PRIMARY KEY id;
CREATE DATASET temp (OpenType) PRIMARY KEY id;
CREATE INDEX idx_fv ON temp(UNNEST forecast SELECT fv : string) EXCLUDE UNKNOWN KEY;

SELECT
    sales.date,
    sales.sid,
    temp.forecast
FROM sales JOIN temp ON temp.x = sales.sid
WHERE (ANY ff IN temp.forecast SATISFIES ff.fv /*+ indexnl */ = "aaaa" END )
LIMIT 10;