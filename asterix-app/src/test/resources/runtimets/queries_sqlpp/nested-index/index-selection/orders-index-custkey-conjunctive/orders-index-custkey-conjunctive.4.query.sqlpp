use tpch;


select element {'o_orderkey':o.nested.o_orderkey,'o_custkey':o.nested.o_custkey}
from  Orders as o
where ((o.nested.o_custkey = 40) and (o.nested.o_totalprice > 150000.0))
order by o.nested.o_orderkey
;
