/*
 * Description  : This test case is to verify the fix for issue343.  It is a more general case.
                : https://code.google.com/p/asterixdb/issues/detail?id=343
 * Expected Res : Success
 * Date         : 30th April 2013
 */

drop  database test if exists;
create  database test;

use test;


create type test.AddressType as
{
  number : int64,
  street : string,
  city : string
}

create type test.AllType as
{
  id : int64,
  name : string,
  age : float,
  salary : double,
  married : boolean,
  interests : {{string}},
  children : [string],
  address : AddressType,
  dob : date,
  time : time,
  datetime : datetime,
  duration : duration,
  location2d : point,
  location3d : point3d,
  line : line,
  polygon : polygon,
  circle : circle
}

create type test.MyListType as
{
  id : int64,
  mylist : [string]
}

create  table "All"(AllType) primary key id;

create  table MyList(MyListType) primary key id;

