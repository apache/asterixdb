      c2 as {{o.orderstatus,o.clerk}},
      c3 as [o.heList,o.openlist,o.loc,o.line,o.poly,o.lastorder],
      c4 as [o.heList,o.openlist,o.loc,o.line,o.poly,o.lastorder]
;
