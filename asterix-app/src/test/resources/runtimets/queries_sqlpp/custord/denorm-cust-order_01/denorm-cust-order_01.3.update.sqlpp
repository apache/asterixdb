use test;


insert into CustomerOrders1
select element {'cid':cid,'cust':cust,'orders':o}
from  Customers1 as c,
      Orders1 as o
where ((c.cid = o.cid) and (c.age < 21) and (c.total > 50.0))
group by c.cid as cid
;
