    OperatorExpr [
      FieldAccessor [
        Variable [ Name=m ]
        Field=author-id
      ]
      =
      LiteralExpr [LONG] [1]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=m ]
        Field=send-time
      ]
      >
      FunctionCall test.datetime@1[
        LiteralExpr [STRING] [2012-08-20T10:10:00]
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=m ]
        Field=send-time
      ]
      <
      FunctionCall test.datetime@1[
        LiteralExpr [STRING] [2012-11-20T10:10:00]
      ]
