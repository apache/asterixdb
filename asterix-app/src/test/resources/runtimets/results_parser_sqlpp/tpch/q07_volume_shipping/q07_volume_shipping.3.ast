  Variable [ Name=t ]
]
LetVariable [ Name=l_year0 ]
  :=
  FunctionCall tpch.get-year@1[
    FieldAccessor [
      Variable [ Name=locs ]
      Field=l_shipdate
    ]
  ]
Where
