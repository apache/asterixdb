DataverseUse tpch
FunctionDecl q7_volume_shipping_tmp([]) {
  (
    SELECT ELEMENT [
    RecordConstructor [
      (
        LiteralExpr [STRING] [supp_nation]
        :
        FieldAccessor [
          Variable [ Name=n1 ]
          Field=n_name
        ]
      )
      (
        LiteralExpr [STRING] [cust_nation]
        :
        FieldAccessor [
          Variable [ Name=n2 ]
          Field=n_name
        ]
      )
      (
        LiteralExpr [STRING] [s_nationkey]
        :
        FieldAccessor [
          Variable [ Name=n1 ]
          Field=n_nationkey
        ]
      )
      (
        LiteralExpr [STRING] [c_nationkey]
        :
        FieldAccessor [
          Variable [ Name=n2 ]
          Field=n_nationkey
        ]
      )
    ]
    ]
    FROM [      Variable [ Name=Nation ]
      AS
      Variable [ Name=n1 ]
,
      Variable [ Name=Nation ]
      AS
      Variable [ Name=n2 ]
    ]
    Where
      OperatorExpr [
        OperatorExpr [
          FieldAccessor [
            Variable [ Name=n2 ]
            Field=n_name
          ]
          =
          LiteralExpr [STRING] [GERMANY]
        ]
        or
        OperatorExpr [
          FieldAccessor [
            Variable [ Name=n1 ]
            Field=n_name
          ]
          =
          LiteralExpr [STRING] [GERMANY]
        ]
      ]
  )
}

Query:
