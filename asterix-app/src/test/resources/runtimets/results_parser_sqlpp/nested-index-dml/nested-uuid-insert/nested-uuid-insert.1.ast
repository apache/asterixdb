DataverseUse twitter
TypeDecl TweetMessageType [
  closed RecordType {
    id : uuid,
    message-text : string
  }
]
TypeDecl nest [
  closed RecordType {
    nested : TweetMessageType
  }
]
TypeDecl doublenest [
  closed RecordType {
    nested : nest
  }
]
DatasetDecl doublenests(doublenest) partitioned by [[nested, nested, id]] [autogenerated]
