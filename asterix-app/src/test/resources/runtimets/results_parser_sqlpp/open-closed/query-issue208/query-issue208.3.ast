    OperatorExpr [
      FieldAccessor [
        Variable [ Name=t ]
        Field=send-time
      ]
      >=
      FunctionCall OpenSocialNetworkData.datetime@1[
        LiteralExpr [STRING] [2005-04-13T17:17:22]
      ]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=t ]
        Field=send-time
      ]
      <=
      FunctionCall OpenSocialNetworkData.datetime@1[
        LiteralExpr [STRING] [2011-04-13T17:18:22]
      ]
    ]
  ]
