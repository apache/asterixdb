DataverseUse test
TypeDecl Name [
  open RecordType {
    first : string,
    last : string
  }
]
TypeDecl Person [
  open RecordType {
    name : Name
  }
]
TypeDecl TestType [
  open RecordType {
    id : int32,
    name : string,
    locn : point,
    zip : string,
    person : Person
  }
]
DatasetDecl t1(TestType) partitioned by [[id]]
