DataverseUse test
TypeDecl DBLPType [
  open RecordType {
    id : int32,
    dblpid : string,
    title : string,
    misc : string
  }
]
DatasetDecl DBLP(DBLPType) partitioned by [[id]]
WriteOutputTo nc1:rttest/inverted-index-complex_ngram-edit-distance-check-let-panic-nopanic_01.adm
Query:
SELECT ELEMENT [
Variable [ Name=o ]
]
FROM [  FunctionCall Metadata.dataset@1[
    LiteralExpr [STRING] [DBLP]
  ]
  AS
  Variable [ Name=o ]
]
LetVariable [ Name=edb ]
  :=
  FunctionCall test.edit-distance-check@3[
    FieldAccessor [
      Variable [ Name=o ]
      Field=authors
    ]
    LiteralExpr [STRING] [Amihay Motro]
    LiteralExpr [LONG] [5]
  ]
LetVariable [ Name=eda ]
  :=
  FunctionCall test.edit-distance-check@3[
    FieldAccessor [
      Variable [ Name=o ]
      Field=authors
    ]
    LiteralExpr [STRING] [Amihay Motro]
    LiteralExpr [LONG] [3]
  ]
Where
  OperatorExpr [
    IndexAccessor [
      Variable [ Name=edb ]
      Index:       Variable [ Name=edb ]
    ]
    and
    IndexAccessor [
      Variable [ Name=eda ]
      Index:       Variable [ Name=eda ]
    ]
  ]
