/*
 * Description    : Tests that non-datascan unnests are rewritten into joins.
 * Success        : Yes
 */

drop  database test if exists;
create  database test;

use test;


write output to asterix_nc1:"rttest/unnest-to-join_01.adm"
select element y
from  [1,2,3,4,5,6] as x,
      [4,5,6,7,8,9] as y
where (x = y)
;
